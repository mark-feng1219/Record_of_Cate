.search_input {
  height: 90rpx;
  padding: 10rpx;
} 
.search_input .navigator{
  background-color: #fff;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 15rpx;
  color: #999;
  font-size: 32rpx;
}
.swiperTtemBox {
  /* 底部内容样式 */
  height: 100vh;
  overflow: scroll;
  margin: 12rpx 0rpx;
  background: white;
  font-size: 28rpx;
}
.container {
  padding-top: 0;
 }
 .item {
  width: 100%;
  height: 220rpx;
  position: relative;
  display: flex;
  margin: 10rpx 10rpx;
  border-bottom: 1px solid rgb(197, 199, 199);
 }
 .item .number-wrapper {
  padding-top: 25rpx;
  margin-left: 2%;
  flex-direction: column;
  justify-content: space-between;
 }
 .number-wrapper .name {
  font-size: 34rpx;
  font-weight: 600;
  overflow: hidden;
 }
 .number-wrapper .count-wrapper {
  display: flex;
  margin-left: 10rpx;
  font-size: 25rpx;
  font-weight: 250;
 }
 .number-wrapper .count-wrapper .count {
  margin: 0 1rpx 0 1rpx;
  font-size: 30rpx;
 }
 .imag{
  width: 130rpx;
  height: 130rpx;
  padding: 20rpx;
  position: absolute;
  top: -10rpx;
  right: 10rpx;
}